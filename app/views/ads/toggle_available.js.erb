<% if @ad.available %>
  $("#available").html("Vapaa / Vuokrattavissa")
  $("#toggle").html("<%= escape_javascript(link_to("Merkitse varatuksi",  toggle_path(:id => @ad.id), :method => :post, :remote => true)) %>")
<% else %>
  $("#available").html("Parhaillaan vuokrassa")
    $("#toggle").html("<%= escape_javascript(link_to("Merkitse vapaaksi",  toggle_path(:id => @ad.id), :method => :post, :remote => true)) %>")
<% end %>
